<odoo>
	<record id="pdf_document_data_tree_view" model="ir.ui.view">
		<field name='name'>pdf.document.data.tree</field>
		<field name="model">pdf.document.data</field>
		<field name="arch" type="xml">
			<tree>
				<field name="state"/>
				<field name="pdf_id" />
				<field name="pdf_data" />
			</tree>
		</field>
	</record>

	<record id="action_pdf_document_data" model="ir.actions.act_window">
		<field name="name">PDF Document Data</field>
		<field name="res_model">pdf.document.data</field>
		<field name="view_mode">tree,form</field>
	</record>
	
	<menuitem id="pdf_document_data_menu" name="PDF Document Data" action="action_pdf_document_data" parent="pdf_document_menu_root" />
	
</odoo>