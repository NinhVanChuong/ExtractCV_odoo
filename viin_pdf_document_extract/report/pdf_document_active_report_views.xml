<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="pdf_document_activity_report_view_graph" model="ir.ui.view">
        <field name="name">pdf.document.active.report.graph</field>
        <field name="model">pdf.document.active.report</field>
        <field name="arch" type="xml">
            <graph string="Pipeline Analysis" sample="1">
                    <field name="date_start" interval="day"/>
                    <field name="date_closed" interval="day"/>
                    <field name="pdf_document_id"/>
                </graph>
        </field>
    </record>
    <record id="pdf_document_activity_report_view_action" model="ir.actions.act_window">
        <field name="name">Activity report</field>
       <field name="res_model">pdf.document.active.report</field>
       <field name="view_mode">tree,form,graph</field>
    </record>
    
    <menuitem id="pdf_document_active_report_menu" name="PDF Document Report" action="pdf_document_activity_report_view_action" parent="pdf_document_menu_root" />
</odoo>